    "type-check": "tsc --noEmit",
    "cleanup:pages": "node ./scripts/cleanup-pages.js",
    "cleanup:pages:powershell": "powershell -ExecutionPolicy Bypass -File ./scripts/cleanup-pages.ps1",
    "cleanup:pages:bash": "bash ./scripts/cleanup-pages.sh",
    "cleanup:pages:cross": "bash ./scripts/cleanup-pages-cross.sh",
    "update-components": "cd C:\\Users\\o_leg\\source\\repos\\pokenae.Web\\pokenae.WebComponent && git pull origin main && npm install && cd c:\\Users\\o_leg\\source\\repos\\pokenae.Web"
  },
  "dependencies": {
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "^5.9.3"
  }
}
